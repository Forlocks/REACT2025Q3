{
  "name": "rs-react-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "tsc -b && vite build",
    "lint": "eslint \"src/**/*.{tsx,ts}\"",
    "lint:fix": "eslint \"src/**/*.{tsx,ts}\" --fix",
    "prettier": "prettier \"src/**/*.{tsx,ts}\" --check",
    "format:fix": "prettier \"src/**/*.{tsx,ts}\" --write",
    "prepare": "husky && echo npm run lint-staged > .husky/pre-commit && echo npm run test > .husky/pre-push",
    "lint-staged": "lint-staged",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --quiet"
    ]
  },
  "dependencies": {
    "lint-staged": "^15.4.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.5.0",
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-compiler": "^19.0.0-beta-55955c9-20241229",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "prettier": "3.4.2",
    "sass-embedded": "^1.83.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vitest": "^3.2.4"
  }
}
